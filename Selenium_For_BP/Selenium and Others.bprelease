	Message = e.Message
Finally
	sw = Nothing
	ss = Nothing
	dw = Nothing
	ds = Nothing
	excel = Nothing
	sheet = Nothing
	source = Nothing
	cells = Nothing
	cell = Nothing
End Try]]></code></stage><stage stageid="30dc3ade-141f-4304-a66d-bb617df28fe3" name="Block1" type="Block"><subsheetid>eb349e0c-c721-4700-bfa3-bef27fe7ee65</subsheetid><loginhibit /><display x="-75" y="-90" w="285" h="75" /><font family="Segoe UI" size="10" style="Regular" color="7FB2E5" /></stage><stage stageid="27b485a6-ec44-44f6-b40a-8c61422e469c" name="Recover1" type="Recover"><subsheetid>eb349e0c-c721-4700-bfa3-bef27fe7ee65</subsheetid><loginhibit /><display x="165" y="-60" /><onsuccess>69582a7b-04e3-445d-847e-f3076e74e160</onsuccess></stage><stage stageid="69582a7b-04e3-445d-847e-f3076e74e160" name="Resume1" type="Resume"><subsheetid>eb349e0c-c721-4700-bfa3-bef27fe7ee65</subsheetid><loginhibit /><display x="165" y="15" /><onsuccess>0ea0e9f0-bff7-43cb-8aa2-05340bc95ab2</onsuccess></stage><stage stageid="3dab5eb5-9fb5-48a5-93c9-d89aab2574c8" name="Input Variables" type="Block"><subsheetid>eb349e0c-c721-4700-bfa3-bef27fe7ee65</subsheetid><loginhibit /><display x="-465" y="-45" w="180" h="180" /><font family="Segoe UI" size="10" style="Regular" color="FFFF00" /></stage><stage stageid="b7dcae48-9ab8-484f-b5f5-2a07370aa11d" name="Local Variables" type="Block"><subsheetid>eb349e0c-c721-4700-bfa3-bef27fe7ee65</subsheetid><loginhibit /><display x="-360" y="150" w="180" h="60" /><font family="Segoe UI" size="10" style="Regular" color="FFFFFF" /></stage><stage stageid="41556207-0744-4e3d-878d-2f660c8fd457" name="Output Variables" type="Block"><subsheetid>eb349e0c-c721-4700-bfa3-bef27fe7ee65</subsheetid><loginhibit /><display x="-270" y="-45" w="180" h="180" /><font family="Segoe UI" size="10" style="Regular" color="FF9900" /></stage><stage stageid="d7f047d0-f277-430a-9896-9cecc3c4e921" name="CopyChartToClipboard" type="SubSheetInfo"><subsheetid>68ca639b-9227-4fbb-8227-e8e824fde38f</subsheetid><narrative>Copies an image of an Excel Chart to the Clipboard so that it can be pasted into another Office Object (Excel, Word, PowerPoint).</narrative><display x="-255" y="-60" w="3